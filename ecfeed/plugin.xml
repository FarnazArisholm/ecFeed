<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="4.2?>
<plugin>
   <extension-point id="com.testify.ecfeed.generator" name="Test Data Generator" schema="schema/com.testify.ecfeed.generator.exsd"/>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="ecFeed"
            id="com.testify.ecfeed">
      </category>
      <wizard
            name="New Equivalence Class Tree file"
            icon="icons/root_node.gif"
            category="com.testify.ecfeed"
            class="com.testify.ecfeed.ui.wizards.NewEcFileWizard"
            id="com.testify.ecfeed.ui.wizards.NewEcFileWizard">
      </wizard>
   </extension>
   
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.testify.ecfeed.ui.editor.EcMultiPageEditor"
            extensions="ect"
            icon="icons/root_node.gif"
            id="com.testify.ecfeed.ui.editors.EcMultiPageEditor"
            name="ecmodel">
      </editor>
   </extension>
</plugin>
